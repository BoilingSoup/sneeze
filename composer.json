{
  "name": "boilingsoup/sneeze",
  "description": "A Laravel Sanctum token-based auth starter kit",
  "keywords": [
    "laravel",
    "auth",
    "sneeze"
  ],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/boilingsoup/sneeze/issues",
    "source": "https://github.com/boilingsoup/sneeze"
  },
  "authors": [
    {
      "name": "Boiling Soup",
      "email": "boilingsoupdev@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2.0",
    "illuminate/console": "^11.0|^12.0",
    "illuminate/filesystem": "^11.0|^12.0",
    "illuminate/support": "^11.0|^12.0",
    "illuminate/validation": "^11.0|^12.0",
    "symfony/console": "^7.0"
  },
  "require-dev": {
    "laravel/framework": "^11.0|^12.0",
    "orchestra/testbench-core": "^9.0|^10.0",
    "phpstan/phpstan": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "BoilingSoup\\Sneeze\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "BoilingSoup\\Sneeze\\SneezeServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
